/amazon-clone/src/Components/Home.js",[],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Components/Header.js",["61","62"],"import React from \"react\";\nimport \"../styles/Header.css\";\nimport SearchIcon from \"@material-ui/icons/Search\";\nimport ShoppingBasketIcon from \"@material-ui/icons/ShoppingBasket\";\nimport { Link } from \"react-router-dom\";\nimport { useStateValue } from \"../StateProvider\";\nimport { auth } from \"../Firebase\";\n\nfunction Header() {\n  const [{ basket, user }, dispatch] = useStateValue();\n\n  const handleAuthenticaton = () => {\n    if (user) {\n      auth.signOut();\n    }\n  };\n\n  return (\n    <div className=\"header\">\n      <Link to=\"/\">\n        <img\n          className=\"header__logo\"\n          src=\"http://pngimg.com/uploads/amazon/amazon_PNG11.png\"\n        />\n      </Link>\n\n      <div className=\"header__search\">\n        <input className=\"header__searchInput\" type=\"text\" />\n        <SearchIcon className=\"header__searchIcon\" />\n      </div>\n\n      <div className=\"header__nav\">\n        <Link to={!user && \"/login\"}>\n          <div onClick={handleAuthenticaton} className=\"header__option\">\n            <span className=\"header__optionLineOne\">\n              Hello {!user ? \"Guest\" : user.email}\n            </span>\n            <span className=\"header__optionLineTwo\">\n              {user ? \"Sign Out\" : \"Sign In\"}\n            </span>\n          </div>\n        </Link>\n\n        <div className=\"header__option\">\n          <span className=\"header__optionLineOne\">Returns</span>\n          <span className=\"header__optionLineTwo\">& Orders</span>\n        </div>\n\n        <div className=\"header__option\">\n          <span className=\"header__optionLineOne\">Your</span>\n          <span className=\"header__optionLineTwo\">Prime</span>\n        </div>\n\n        <Link to=\"/checkout\">\n          <div className=\"header__optionBasket\">\n            <ShoppingBasketIcon />\n            <span className=\"header__optionLineTwo header__basketCount\">\n              {basket?.length}\n            </span>\n          </div>\n        </Link>\n      </div>\n    </div>\n  );\n}\n\nexport default Header;\n","/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Components/Product.js",["63"],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/reducer.js",[],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/StateProvider.js",[],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Components/Login.js",["64"],"import React, { useState } from \"react\";\nimport \"../styles/Login.css\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { auth } from \"../Firebase\";\n\nfunction Login() {\n  const history = useHistory();\n\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n  const signIn = (e) => {\n    e.preventDefault();\n\n    auth\n      .signInWithEmailAndPassword(email, password)\n      .then((auth) => {\n        history.push(\"/\");\n      })\n      .catch((error) => alert(error.message));\n  };\n\n  const register = (e) => {\n    e.preventDefault();\n\n    auth\n      .createUserWithEmailAndPassword(email, password)\n      .then((auth) => {\n        // it successfully created a new user with email and password\n        if (auth) {\n          history.push(\"/\");\n        }\n      })\n      .catch((error) => alert(error.message));\n  };\n\n  return (\n    <div className=\"login\">\n      <Link to=\"/\">\n        <img\n          className=\"login__logo\"\n          src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/a/a9/Amazon_logo.svg/1024px-Amazon_logo.svg.png\"\n        />\n      </Link>\n\n      <div className=\"login__container\">\n        <h1>Sign-in</h1>\n\n        <form>\n          <h5>E-mail</h5>\n          <input\n            type=\"text\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n          />\n\n          <h5>Password</h5>\n          <input\n            type=\"password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n\n          <button\n            type=\"submit\"\n            onClick={signIn}\n            className=\"login__signInButton\"\n          >\n            Sign In\n          </button>\n        </form>\n\n        <p>\n          By signing-in you agree to the AMAZON FAKE CLONE Conditions of Use &\n          Sale. Please see our Privacy Notice, our Cookies Notice and our\n          Interest-Based Ads Notice.\n        </p>\n\n        <button onClick={register} className=\"login__registerButton\">\n          Create your Amazon Account\n        </button>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/firebase.js",[],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Firebase.js",[],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Components/CheckoutProduct.js",["65","66","67","68"],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Components/Subtotal.js",["69","70"],"/Users/rahettuteja/Desktop/WebD/Basics/React/amazon-clone/src/Components/Checkout.js",["71"],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":12,"column":10,"nodeType":"78","messageId":"79","endLine":12,"endColumn":15},{"ruleId":"80","severity":1,"message":"81","line":35,"column":6,"nodeType":"82","endLine":35,"endColumn":8,"suggestions":"83"},{"ruleId":"76","severity":1,"message":"84","line":10,"column":28,"nodeType":"78","messageId":"79","endLine":10,"endColumn":36},{"ruleId":"85","severity":1,"message":"86","line":21,"column":9,"nodeType":"87","endLine":24,"endColumn":11},{"ruleId":"76","severity":1,"message":"88","line":7,"column":12,"nodeType":"78","messageId":"79","endLine":7,"endColumn":18},{"ruleId":"85","severity":1,"message":